<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.douyin.dao.mapper.UsersMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.douyin.model.domain.Users">
        <id column="id" property="id" />
        <result column="mobile" property="mobile" />
        <result column="nickname" property="nickname" />
        <result column="douyin_num" property="douyinNum" />
        <result column="face" property="face" />
        <result column="sex" property="sex" />
        <result column="birthday" property="birthday" />
        <result column="country" property="country" />
        <result column="province" property="province" />
        <result column="city" property="city" />
        <result column="district" property="district" />
        <result column="description" property="description" />
        <result column="bg_img" property="bgImg" />
        <result column="can_douyin_num_be_updated" property="canDouyinNumBeUpdated" />
        <result column="created_time" property="createdTime" />
        <result column="updated_time" property="updatedTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, mobile, nickname, douyin_num, face, sex, birthday, country, province, city, district, description, bg_img, can_douyin_num_be_updated, created_time, updated_time
    </sql>

</mapper>
